        <script>
            function say(){
                for(i=0;i<10;i++){
                    document.write(i+"<br>");
                }

            }
            say();
            function say1(j){
                for(i=0;i<j;i++){
                    document.write(i+"<br>");
                }
                }
            say1(50);
        </script>
    </body>
